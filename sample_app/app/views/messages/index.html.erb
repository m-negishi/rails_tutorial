<div class="row">
  <% provide(:title, 'Message') %>
  <h1>Message</h1>
  <div class="message_feed">
    <% if @messages.any? %>
      <ol class="messages">
        <%= render partial: 'message', collection: @messages %>
      </ol>
      <%= will_paginate @messages %>
    <% end %>
  </div>
</div>
